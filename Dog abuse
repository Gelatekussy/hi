	--//=================================\\
	--|| 	       ğ™‚ğ™ğ™„ ğ™ğ™ğ™‰ğ˜¾ğ™ğ™„ğ™Šğ™‰
	--\\=================================//

	local function CircleTween()
		Circle.Size = UDim2.new(0.203999996, 0, 0.400000006, 0)
		Circle.Position = UDim2.new(0.401353776, 0, 0.0103071332, 0)
		Circle.Visible = true
		Circle:TweenSize(UDim2.new(1.2, 0,1.2, 0), "In", "Quart", .25)
		Circle:TweenPosition(UDim2.new(-0.096, 0,-0.388, 0), "In", "Quart", .25)
		wait(.25)
		Circle.Visible = false
		Circle.Size = UDim2.new(0.203999996, 0, 0.400000006, 0)
		Circle.Position = UDim2.new(0.401353776, 0, 0.0103071332, 0)
	end

	local function NormalModeF()
		TextLabel.Text = "Mode: Normal"
		CircleTween()
		startPos()
		NormalMode = true
		WeirdMode = false
		BrutalMode = false
	end

	local function WeirdModeF()
		TextLabel.Text = "Mode: Weird"
		CircleTween()
		startPos()
		WeirdMode = true
		NormalMode = false
		BrutalMode = false
	end

	local function BrutalModeF()
		TextLabel.Text = "Mode: Brutal"
		CircleTween()
		startPos()
		BrutalMode = true
		WeirdMode = false
		NormalMode = false
	end

	--//=================================\\
	--|| 	     ğ™ˆğ™Šğ˜¿ğ™€ ğ™ğ™’ğ™„ğ™ğ˜¾ğ™ƒğ™„ğ™‰ğ™‚
	--\\=================================//

--[[

Keybinds:

B - Normal Mode
N - Weird Mode
M - Brutal Mode

]]--

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "b" then
			if NormalMode == true then

			else
				NormalModeF()
			end
		end
	end)

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "n" then
			if WeirdMode == true then

			else
				WeirdModeF()
			end
		end
	end)

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "m" then
			if BrutalMode == true then

			else
				BrutalModeF()
			end
		end
	end)

	--//=================================\\
	--|| 	   ğ™’ğ˜¼ğ™‡ğ™†ğ™„ğ™‰ğ™‚ ğ™ˆğ™Šğ™‘ğ™€ğ™ˆğ™€ğ™‰ğ™
	--\\=================================//

	local walk = coroutine.create(function()
		while wait(.2) do
			if walking1 == true then
				if walking1 == false then startPos() end
				if WeirdMode or BrutalMode == true then end 
				WalkingPos1()
				if walking1 == false then startPos() end
				wait(.2)
				if walking1 == false then startPos() end
				if WeirdMode or BrutalMode == true then end 
				StandingF()
				if walking1 == false then startPos() end
				wait(.2)
				if walking1 == false then startPos() end
				if WeirdMode or BrutalMode == true then end 
				WalkingPos2()
				if walking1 == false then startPos() end
			end
		end
	end)
	coroutine.resume(walk)

	Character.Humanoid.Running:Connect(function(h)
		if h >= 2 then
			if WeirdMode or BrutalMode == true then return end 
			walking1 = true
		else
			if WeirdMode or BrutalMode == true then return end 
			startPos()
			walking1 = false
		end
	end)


	--//=================================\\
	--|| 	     ğ™†ğ™€ğ™”ğ˜½ğ™„ğ™‰ğ˜¿ ğ˜¼ğ˜¾ğ™ğ™„ğ™Šğ™‰
	--\\=================================//

	-- Normal Mode

--[[

E - Stand
R - TBag

]]--

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "e" then
			if NormalMode == true then
				if Standing == false then
					if WeirdMode or BrutalMode == true then return end
					Standing = true
					StandingF()
				else
					Standing = false
					startPos()
				end
			end
		end
	end)

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "r" then
			if NormalMode == true then
				if WeirdMode or BrutalMode == true then return end
				tBag()
				wait(.25)
				StandingF()
			end
		end
	end)

	-- Weird Mode

--[[

E - Fck
R - Gauk

]]--

	wait()

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "e" then
			if WeirdMode == true then
				if NormalMode or BrutalMode == true then return end
				f2()
				wait(.2)
				f1()
			end
		end
	end)

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "r" then
			if WeirdMode == true then
				if NormalMode or BrutalMode == true then return end
				gauk()
			end
		end
	end)

	-- Brutal Mode

--[[

E - Stab
R - Choke
Y - Punch
U - Slit

]]--

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "e" then
			if BrutalMode == true then
				if NormalMode or WeirdMode == true then return end
				Stab()
				wait(.7)
				Holding()
			end
		end
	end)

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "r" then
			if BrutalMode == true then
				if NormalMode or WeirdMode == true then return end
				Choke1()
				wait(.2)
				Choke2()
			end
		end
	end)

	wait()

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "y" then
			if BrutalMode == true then
				if NormalMode or WeirdMode == true then return end
				Punch1()
				wait(.1)
				Punch2()
				wait(.1)
				Punch3()
				wait(.1)
				Punch1()
			end
		end
	end)

	wait()

	m.KeyDown:connect(function(k)
		k = k:lower()
		if k == "u" then
			if BrutalMode == true then
				if NormalMode or WeirdMode == true then return end
				SlitHold()
				wait(.35)
				Slitting()
				wait(1)
				SlitHold()
			end
		end
	end)
